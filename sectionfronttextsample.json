            }
          ],
          "image": {
            "type": "image",
            "image_path": "https://cdn.newsday.com/ace/c:Nzk5ODgwZDYtYmYzZS00:MTRhNzE4/preview/640",
            "image_path_large": "https://cdn.newsday.com/ace/c:Nzk5ODgwZDYtYmYzZS00:MTRhNzE4/landscape/640",
