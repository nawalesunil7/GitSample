            "type": "image",
            "image_path": "https://cdn.newsday.com/ace/c:MjdhMmJhNWQtYmRiMS00:MjQ5NzNh/preview/640",
            "image_path_large": "https://cdn.newsday.com/ace/c:MjdhMmJhNWQtYmRiMS00:MjQ5NzNh/landscape/640",
