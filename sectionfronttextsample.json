            }
          ],
          "image": {
            "type": "image",
            "image_path": "https://cdn.newsday.com/ace/c:ODc3OTg5NmQtZTExYi00:OTQ2ZDc4/preview/640",
            "image_path_large": "https://cdn.newsday.com/ace/c:ODc3OTg5NmQtZTExYi00:OTQ2ZDc4/landscape/640",
